<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
	
	
	
	
	<managed-bean>
		<managed-bean-name>patientController</managed-bean-name>
		<managed-bean-class>com.ensah.web.mb.PatientController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>patientService</property-name>
			<property-class>com.ensah.services.PatientService</property-class>
			<value>#{patientService}</value>
		</managed-property>
	</managed-bean>
	
	
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
</faces-config>
